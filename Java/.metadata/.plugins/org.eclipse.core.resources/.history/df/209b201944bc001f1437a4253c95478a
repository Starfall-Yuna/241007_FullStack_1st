import java.util.Scanner;

public class double_loop {
	public static void main(String args[]) {
		Scanner scan = new Scanner(System.in);
		int n = scan.nextInt();
		
		for(int i=1; i<=n; i++) {		// i :: 1부터 n까지
			for(int j=1; j<=n; j++) {	// j :: 1부터 n까지
				System.out.print("*");
			}
			// System.out.println();
		}
	}
}
