
public class operator {
	public static void main(String args[]) {
		// 산술연산자 :: +, -, *, /, %
		// 대입연산자 :: =
		//		(+=, -=, *=, /=, %=)
		// 논리연산자 :: (AND)&&, & / (OR)||, |
		boolean tf = 2==2 | 5!=1;
			// | :: 첫번째 항(2==2), 두번째 항(5!=1) 모두 따짐
			// 
		// 비교연산자 :: <, >, <=, >=, ==, !=
		// 증감연산자 :: ++, --
		// 삼항연산자 :: ?
	}
}
